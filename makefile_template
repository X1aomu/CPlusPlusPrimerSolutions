# Thie file is the template for all Makefiles
# DO NOT DELETE THIS FILE

CXX = g++
LDFLAGS = -lstdc++ -lm
FLAGS = -pipe -std=c++11 -pedantic-errors -Wall -Wextra -ggdb
INC = -I..
CXXFLAGS = $(FLAGS) $(INC)

all: $(TARGETS)

%.o: %.h

clean:
	find -iname "*un~" -delete
	rm -f *.o

clobber: clean
	rm -f $(TARGETS)

.PHONY: all
