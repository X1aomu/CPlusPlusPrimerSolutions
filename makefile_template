# Thie file is the template for all Makefiles
# DO NOT DELETE THIS FILE

CC = g++
CCFLAGS = -pipe -std=c++11 -pedantic-errors -Wall -Wextra -ggdb -lm
INCLUDES =

CC_SRCS = $(wildcard *.cc)
CC_BINS = $(patsubst %.cc, %, $(CC_SRCS))

.PHONY: all

all: $(CC_BINS)

%: %.cc
	-$(CC) $(CCFLAGS) $(INCLUDES) -o $@ $<

clean:
	rm -f *\~ .*.un\~

clobber: clean
	rm -f $(CC_BINS)
